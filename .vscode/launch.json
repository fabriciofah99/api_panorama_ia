{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "FastAPI Debug",
            "type": "python",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "main:app",
                "--host",
                "127.0.0.1",
                "--port",
                "8080",
                "--reload"
            ],
            "jinja": true,
            "envFile": "${workspaceFolder}/.env",
            "console": "integratedTerminal"
        }
    ]
}